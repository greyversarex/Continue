modules = ["web", "nodejs-20", "php-8.2"]
run = "npm run start"
[agent]
expertMode = true
integrations = ["javascript_database==1.0.0==1.0.0:1.0.0", "javascript_object_storage:1.0.0", "javascript_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["sqlite"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Unified Server"

[[workflows.workflow]]
name = "Unified Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"
waitForPort = 5000

[[ports]]
localPort = 80
externalPort = 3003

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 5555
externalPort = 4200

[[ports]]
localPort = 8000
externalPort = 80

[[ports]]
localPort = 8001
externalPort = 3002

[[ports]]
localPort = 38803
externalPort = 5173

[deployment]
deploymentTarget = "autoscale"
run = ["node", "index.js"]
